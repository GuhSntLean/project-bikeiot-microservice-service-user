{
  "name": "userservices",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev src/server/server.ts",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/config/AppDataSource.ts migration:generate",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/config/AppDataBase.ts migration:run",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli",
    "typeorm-migration": "yarn exec -- ts-node node_modules/typeorm/cli migration:revert -d ./src/config/AppDataBase.ts"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.10",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.4",
    "@types/uuid": "^8.3.4",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
